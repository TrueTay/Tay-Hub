    print("Clicked")
end) 
local Tab = Window:NewTab("Settings")
local SettingsSection = Tab:NewSection("Settings")
SettingsSection:NewKeybind("KeybindText", "KeybindInfo", Enum.KeyCode.G, function() 
	Library:ToggleUI()
end)

