local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()

local Window = Library.CreateLib("Tay Hub", "Midnight")  --You can change the theme to      LightTheme
--DarkTheme
   -- GrapeTheme
   -- BloodTheme
   -- Ocean
   -- Midnight
   -- Sentinel
    --Synapse

local Tab = Window:NewTab("Home")  
local HomeSection = Tab:NewSection("Welcome To Tay Hub")
local HomeSection = Tab:NewSection("Made By Tayy#0071")
local HomeSection = Tab:NewSection("Add me on discord if there is a problem.")
local HomeSection = Tab:NewSection("if you have any suggestions to add send me a dm.")

local Tab = Window:NewTab("Main")
local MainSection = Tab:NewSection("Main")
MainSection:NewSlider("WalkSpeed", "Speed When you use shiftlock it remove ", 500, 0, function(s) 
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)

MainSection:NewSlider("JumpPower", "JumpPower Hack", 500, 50, function(s) 
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = s
end)
MainSection:NewButton("Teleport", "ctrl + click", function()
    loadstring(game:HttpGet("https://pastebin.com/raw/sAmc4YvK"))()
    print("Clicked")
end)
MainSection:NewButton("Noclip Gui", "A noclip Gui", function()
    loadstring(game:HttpGet("https://pastebin.com/raw/9LBsfRkD"))()
    print("Clicked")
end)
MainSection:NewDropdown("Give Gun", "Gives the localplayer a gun only on prison life", {"M9", "Remington 870", "AK-47"}, function(v)
        local A_1 = game:GetService("Workspace")["Prison_ITEMS"].giver[v].ITEMPICKUP
        local Event = game:GetService("Workspace").Remote.ItemHandler
        Event:InvokeServer(A_1)
    end)
